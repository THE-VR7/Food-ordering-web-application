        onload = start;
        function start() {
            var im1 = "linear-gradient(rgba(0,0,0,0.6),rgba(0,0,0,0.6)),url('https://raw.githubusercontent.com/THE-VR7/Food-ordering-web-application/master/images/banner12.jpg')"
            var im2 = "linear-gradient(rgba(0,0,0,0.6),rgba(0,0,0,0.6)),url('https://raw.githubusercontent.com/THE-VR7/Food-ordering-web-application/master/images/img3.jpg')"
            var im3 = "linear-gradient(rgba(0,0,0,0.6),rgba(0,0,0,0.6)),url('https://raw.githubusercontent.com/THE-VR7/Food-ordering-web-application/master/images/img2.jpg')"
            var im4 = "linear-gradient(rgba(0,0,0,0.6),rgba(0,0,0,0.6)),url('https://raw.githubusercontent.com/THE-VR7/Food-ordering-web-application/master/images/img-5.jpg')"
            var im5 = "linear-gradient(rgba(0,0,0,0.6),rgba(0,0,0,0.6)),url('https://raw.githubusercontent.com/THE-VR7/Food-ordering-web-application/master/images/img-4.jpg')"
            var im6 = "linear-gradient(rgba(0,0,0,0.6),rgba(0,0,0,0.6)),url('https://raw.githubusercontent.com/THE-VR7/Food-ordering-web-application/master/images/slide_2.jpg')"
            var i = 0;
            function Move() {
                i = (i % 6) + 1; // 5 is the Number of image in slider
                if (i == 1) {
                    document.getElementById('img').style.backgroundImage = im1
                }
                else if (i == 2) {
                    document.getElementById('img').style.backgroundImage = im2
                }
                else if (i == 3) {
                    document.getElementById('img').style.backgroundImage = im3
                }
                else if (i == 4) {
                    document.getElementById('img').style.backgroundImage = im4
                }
                else if (i == 5) {
                    document.getElementById('img').style.backgroundImage = im5
                }
                else {
                    document.getElementById('img').style.backgroundImage = im6
                }
            }
            setInterval(Move, 3000); //change img in 3 sec
        }
