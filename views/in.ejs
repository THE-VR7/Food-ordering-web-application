    <script>
        function payment() {
            if (totprice == 0) {
                alert("Please book at least one item to pay")
            }
            else {
                location.href = "";
                document.getElementById("finalpay").value = totprice
            }
        }
        function disableScroll() {
            // Get the current page scroll position 
            scrollTop =
                window.pageYOffset || document.documentElement.scrollTop;
            scrollLeft =
                window.pageXOffset || document.documentElement.scrollLeft,

                // if any scroll is attempted, 
                // set this to the previous value 
                window.onscroll = function () {
                    window.scrollTo(scrollLeft, scrollTop);
                };
        }
        function enableScroll() {
            window.onscroll = function () { };
        }
        var totprice = 0
        function show_cart(x, y, z) {
            document.getElementById("finalcart").style.display = "block";
            location.href = '#cart11';
            document.getElementById("lb11").innerHTML = x;
            document.getElementById("lb22").innerHTML = y;
            document.getElementById("lb33").innerHTML = z;
            totprice = totprice + z;
            document.getElementById("price1").innerHTML = totprice;
        }
        function call_quant() {
            var quant = document.getElementById("quant").value;
            var z2 = parseInt(document.getElementById("lb33").textContent, 10);
            totprice = totprice + z2 * quant - z2;
            document.getElementById("price1").innerHTML = totprice;
        }
        function cancel_item() {
            var z1 = parseInt(document.getElementById("lb33").textContent, 10);
            var y2 = document.getElementById("quant").value
            if (y2 == 0 || totprice == 0) {
                alert("Can't cancel this item anymore")
            }
            else {
                document.getElementById("quant").value = y2 - 1;
                totprice = totprice - z1;
                document.getElementById("price1").innerHTML = totprice;
            }
        }
        function order() {
            location.href = '#menubox';
            document.getElementById("quant").value = 1;

        }

